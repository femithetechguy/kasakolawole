{
  "app": {
    "name": "Casa Kolawole",
    "title": "Casa Kolawole - Dynamic Portfolio & Business Platform",
    "description": "A dynamic, responsive application showcasing Casa Kolawole's work and services",
    "version": "1.0.0",
    "author": "Casa Kolawole",
    "responsive": true,
    "theme": {
      "primaryColor": "#2C3E50",
      "secondaryColor": "#E74C3C",
      "accentColor": "#F39C12",
      "backgroundColor": "#FFFFFF",
      "textColor": "#2C3E50",
      "fontFamily": "Inter, Arial, sans-serif"
    },
    "layout": {
      "headerHeight": "80px",
      "footerHeight": "60px",
      "sidebarWidth": "250px",
      "maxContentWidth": "1200px",
      "breakpoints": {
        "mobile": "768px",
        "tablet": "1024px",
        "desktop": "1200px"
      }
    }
  },
  "navigation": {
    "type": "horizontal",
    "position": "top",
    "sticky": true,
    "logo": {
      "text": "Casa Kolawole",
      "image": "/assets/images/logo.png",
      "alt": "Casa Kolawole Logo"
    },
    "menu": {
      "hamburger": true,
      "mobileBreakpoint": "768px"
    }
  },
  "auth": {
    "enabled": true,
    "type": "firebase",
    "fallback": {
      "username": "admin",
      "password": "admin"
    },
    "redirectAfterLogin": "/home",
    "sessionTimeout": 3600000,
    "rememberMe": true
  },
  "tabs": [
    {
      "id": "landing",
      "name": "Landing",
      "title": "Casa Kolawole - Access Portal",
      "description": "Secure access portal for Casa Kolawole platform",
      "path": "/landing",
      "directory": "landing",
      "icon": "lock",
      "order": 0,
      "visible": false,
      "active": false,
      "isAuth": true,
      "files": {
        "html": "landing/index.html",
        "css": "landing/landing.css",
        "js": "landing/landing.js",
        "json": "landing/landing.json"
      },
      "components": [
        "login-form",
        "branding",
        "footer"
      ],
      "seo": {
        "metaTitle": "Casa Kolawole - Secure Access",
        "metaDescription": "Secure access portal for Casa Kolawole platform",
        "keywords": ["login", "access", "portal", "secure"]
      }
    },
    {
      "id": "home",
      "name": "Home",
      "title": "Welcome to Casa Kolawole",
