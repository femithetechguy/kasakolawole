  "global": {
    "assets": {
      "css": [
        "assets/css/reset.css",
        "assets/css/global.css",
        "assets/css/responsive.css",
        "assets/css/animations.css"
      ],
      "js": [
        "assets/js/app.js",
        "assets/js/navigation.js",
        "assets/js/utils.js",
        "assets/js/animations.js"
      ],
      "images": "assets/images/",
      "fonts": "assets/fonts/"
    },
    "dependencies": {
      "libraries": [
        {
          "name": "animate.css",
          "version": "4.1.1",
          "type": "css",
          "cdn": "https://cdnjs.cloudflare.com/ajax/libs/animate.css/4.1.1/animate.min.css"
        },
        {
          "name": "aos",
          "version": "2.3.4",
          "type": "js",
          "cdn": "https://unpkg.com/aos@2.3.4/dist/aos.js"
        }
      ]
    },
    "features": {
      "lazyLoading": true,
      "animations": true,
      "darkMode": false,
      "multiLanguage": false,
      "analytics": true,
      "seo": true,
      "pwa": false
    }
  },
  "build": {
    "environment": "development",
    "autoGenerate": {
      "enabled": true,
      "onTabUpdate": true,
      "createDirectories": true,
      "generateFiles": true
    },
    "structure": {
      "baseDirectory": "/",
      "tabDirectoryPattern": "{tabId}/",
      "fileNaming": {
        "html": "index.html",
        "css": "{tabId}.css",
        "js": "{tabId}.js",
        "json": "{tabId}.json"
      }
    },
    "templates": {
      "html": "templates/page-template.html",
      "css": "templates/style-template.css",
      "js": "templates/script-template.js",
      "json": "templates/data-template.json"
    }
  },
  "api": {
    "baseUrl": "/api",
    "endpoints": {
      "tabs": "/tabs",
      "content": "/content/{tabId}",
      "navigation": "/navigation",
      "global": "/global-config"
    },
    "methods": {
      "get": ["tabs", "content", "navigation", "global"],
      "post": ["content"],
      "put": ["content", "tabs"],
      "delete": ["content"]
    }
  },
  "social": {
    "links": [
      {
        "platform": "LinkedIn",
